    "mocha-junit-reporter": "1.22.0",
    "tslint": "5.16.0",
    "tslint-microsoft-contrib": "6.1.1",
    "typescript": "3.4.5",
    "webpack": "4.31.0",
    "webpack-cli": "3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/SyncTasks"
